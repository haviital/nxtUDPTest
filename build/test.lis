  8837                             ; driver: zx_01_input_kbd_inkey
  8838                             ; fd    : 0
  8839                             ; mode  : read only
  8840                             ; type  : 001 = input terminal
  8841                             ; tie   : __i_fcntl_fdstruct_1
  8842                             ;
  8843                             ; ioctl_flags   : CRT_ITERM_TERMINAL_FLAGS
  8844                             ; buffer size   : 64 bytes
  8845                             ; debounce      : CRT_ITERM_INKEY_DEBOUNCE ms
  8846                             ; repeat_start  : CRT_ITERM_INKEY_REPEAT_START ms
  8847                             ; repeat_period : CRT_ITERM_INKEY_REPEAT_RATE ms
