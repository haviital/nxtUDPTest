  8884                          
  8885  000b  00                      defb 0         ; thread owner = none
  8886  000c  02                      defb 0x02      ; mtx_recursive
  8887  000d  00                      defb 0         ; lock count = 0
  8888  000e  fe                      defb 0xfe      ; atomic spinlock
  8889  000f  0000                    defw 0         ; list of blocked threads
